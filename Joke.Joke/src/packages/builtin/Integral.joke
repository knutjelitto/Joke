trait Integral<N : Intergal<N>> is Real<N>

    fun quot(that: N): N
    fun rem(that: N): N
    fun div(that: N): N
    fun mod(that: N): N
    fun quot-rem(that: N): (N, N)
    fun div-mod(that: N): (N, N)

    fun even(): bool
    fun odd(): bool
    fun gcd(): bool
    fun lcm(): bool
